{% extends 'PetkoparaCrudGeneratorBundle::base.html.twig' %}

{% block title %}
{{ parent() }} - prof
{% endblock %}

{% block body %}

 

<div class="row">
    <div class="col-md-6">
        <p><strong>Id</strong></p>
        <p>
            {{ prof.id }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Nom</strong></p>
        <p>
            {{ prof.nom }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Prenom</strong></p>
        <p>
            {{ prof.prenom }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Cin</strong></p>
        <p>
            {{ prof.cin }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Datenaissance</strong></p>
        <p>
            {% if prof.dateNaissance %}{{ prof.dateNaissance|date('Y-m-d H:i:s') }}{% endif %}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Genre</strong></p>
        <p>
            {{ prof.genre }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Email</strong></p>
        <p>
            {{ prof.email }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Competences</strong></p>
        <p>
            {{ prof.competences }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Adresse</strong></p>
        <p>
            {{ prof.adresse }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Numerotel</strong></p>
        <p>
            {{ prof.numeroTel }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Debut</strong></p>
        <p>
            {% if prof.debut %}{{ prof.debut|date('Y-m-d H:i:s') }}{% endif %}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Fin</strong></p>
        <p>
            {% if prof.fin %}{{ prof.fin|date('Y-m-d H:i:s') }}{% endif %}
        </p>

    </div></div>

<hr/>


<div class="form-group">
    <a class="btn btn-default" href="{{ path('prof') }}">
        <span class="glyphicon glyphicon-list" aria-hidden="true"></span>
        Retourner vers la liste
    </a>
    <a class="btn btn-primary" href="{{ path('prof_edit', { 'id':  prof.id }) }}">
        <span class="glyphicon glyphicon-pencil" aria-hidden="true"></span>
        Edit
    </a>
    <a class="btn btn-info" href="{{ path('prof_new') }}">
        <span class="glyphicon glyphicon-plus" aria-hidden="true"></span>
        New
    </a>
    <form action="{{ path('prof_delete', { 'id': prof.id }) }}" method="post" style="display: inline-block">
        <input type="hidden" name="_method" value="DELETE" />
        {{ form_widget(delete_form) }}
        <button class="btn btn-danger" type="submit" onclick="return confirm('Are you sure');">
            <span class="glyphicon glyphicon-remove" aria-hidden="true"></span>
            Delete
        </button>
    </form>
</div>


{% endblock %}

